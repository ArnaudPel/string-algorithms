{
  "name": "string-algorithms",
  "version": "1.0.2",
  "description": "Linear-time string search and comparison algorithms.",
  "author": "Kim Burgaard <kim@burgaard.us> (http://www.burgaard.us)",
  "license": "MIT",
  "keywords": [
    "suffix",
    "array",
    "suffix",
    "tree",
    "longest",
    "common",
    "substring",
    "longest",
    "common",
    "lcp",
    "linear",
    "time",
    "o(n)",
    "algorithm"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint src/**/*.js",
    "test": "jest --coverage",
    "test-debug": "BABEL_ENV=dev node --inspect ./node_modules/.bin/jest --runInBand",
    "build": "eslint src/**/*.js && jest && babel src --out-dir lib --ignore spec.js"
  },
  "devDependencies": {
    "babel": "6.23.0",
    "babel-cli": "6.24.1",
    "babel-jest": "20.0.3",
    "babel-preset-es2015": "6.24.1",
    "eslint": "4.4.1",
    "eslint-config-airbnb-base": "11.3.1",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-jest": "20.0.3",
    "jest": "20.0.4"
  },
  "dependencies": {},
  "jest": {
    "testPathIgnorePatterns": [
      "node_modules",
      "lib"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/lib/**",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageReporters": [
      "text"
    ]
  }
}
